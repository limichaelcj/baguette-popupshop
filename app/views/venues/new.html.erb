<div class="container" style="color: white;">
  <h2>New Venue</h2>
    <%= simple_form_for @venue do |f| %>
      <%= f.input :name %>
      <%= f.input :address %>
      <%= f.input :description %>
      <%= f.input :price %>
      <%= f.button :submit, class: "btn btn-primary" %>
    <% end %></div>
</div>
